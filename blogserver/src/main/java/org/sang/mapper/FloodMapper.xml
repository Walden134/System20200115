<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.sang.mapper.FloodMapper">
	<select id="getBaseP" resultType="org.sang.bean.flood.BaseP">
		SELECT * FROM `basep`
	</select>
	<select id="getFuturePByRcp"
		resultType="org.sang.bean.flood.FutureP">
		SELECT * FROM `futurep`where rcpId=#{rcpId}
	</select>
	<select id="getBaseFlood"
		resultType="org.sang.bean.flood.BaseFlood">
		SELECT year,q,w1,w3,w7,w15,p FROM `baseflood`
	</select>
</mapper>